    "import numpy as np\n",
    "\n",
    "is_prime = np.ones((100,), dtype=bool)\n",
    "is_prime[:2] = 0\n",
    "\n",
    "N_max = int(np.sqrt(len(is_prime) - 1))\n",
    "print(N_max)\n",
    "\n",
    "for j in range(2, N_max + 1):\n",
    "    is_prime[2*j::j] = False\n",
    "\n",
    "\n",
    "is_prime"
